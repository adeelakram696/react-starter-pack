{
  "name": "react-starter-pack",
  "version": "0.1.0",
  "author": {
    "name": "Adeel Akram",
    "url": "https://github.com/adeelakram696/react-starter-pack"
  },
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "history": "4.9.0",
    "node-sass-chokidar": "1.3.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-app-rewire-eslint": "0.2.3",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
    "react-scripts-rewired": "2.1.8",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "lint": "eslint src --ext .js --ext .jsx --no-cache --quiet",
    "lintfix": "eslint src --ext .js --ext .jsx --no-cache --fix",
    "build-css": "node-sass-chokidar src/styles/scss/App.scss -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss/App.scss -o src/styles --w --recursive"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "17.1.0",
    "sinon": "7.3.2"
  }
}
