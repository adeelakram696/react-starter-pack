{
  "name": "react-starter-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "axios": "0.18.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "history": "4.7.2",
    "node-sass-chokidar": "1.3.4",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts-rewired": "2.1.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "lint": "eslint src --ext .js --ext .jsx --no-cache --quiet",
    "lintfix": "eslint src --ext .js --ext .jsx --no-cache --fix",
    "build-css": "node-sass-chokidar src/styles/scss/App.scss -o src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/scss/App.scss -o src/styles --w --recursive"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-jest": "23.6.0",
    "babel-plugin-import": "1.11.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "23.6.0",
    "sinon": "7.1.1"
  }
}
